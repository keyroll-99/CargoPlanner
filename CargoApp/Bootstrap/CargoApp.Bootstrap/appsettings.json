{
  "Jwt": {
    
  },
  "Rabbit": {
  },
  "Mail": {
    
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.Seq"],
    "MinimumLevel": {
      "Default": "Warning"
    },
    "WriteTo": [
      {
        "Name": "Console"
      }, 
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
    }],
    "Enrich": ["FromLogContext", "WithThreadId", "WithMachineName"]
  },
  "AllowedHosts": "*"
}
